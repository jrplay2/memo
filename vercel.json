{
  "version": 2,
  "rewrites": [
    { "source": "/cod/1", "destination": "/cod/1/index.html" },
    { "source": "/cod", "destination": "/cod/index.html" }
  ],
  "headers": [
    {
      "source": "/novas_imagens/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}